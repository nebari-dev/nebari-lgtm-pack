{
  "annotations": {
    "list": []
  },
  "description": "Envoy Gateway traffic metrics for Nebari routing",
  "editable": false,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Request Rate by Response Code",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "uid": "mimir", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": { "mode": "normal" }
          },
          "unit": "reqps"
        },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": "2xx" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": "4xx" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": "5xx" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "targets": [
        {
          "expr": "sum(rate(envoy_http_downstream_rq_xx{envoy_response_code_class=\"2\"}[5m]))",
          "legendFormat": "2xx",
          "refId": "A"
        },
        {
          "expr": "sum(rate(envoy_http_downstream_rq_xx{envoy_response_code_class=\"4\"}[5m]))",
          "legendFormat": "4xx",
          "refId": "B"
        },
        {
          "expr": "sum(rate(envoy_http_downstream_rq_xx{envoy_response_code_class=\"5\"}[5m]))",
          "legendFormat": "5xx",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "datasource": { "uid": "mimir", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(envoy_http_downstream_cx_active)",
          "legendFormat": "Active",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "datasource": { "uid": "mimir", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(increase(envoy_http_downstream_rq_total[24h]))",
          "legendFormat": "Total (24h)",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Request Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "uid": "mimir", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "ms"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(envoy_http_downstream_rq_time_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(envoy_http_downstream_rq_time_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(envoy_http_downstream_rq_time_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Bytes In / Out",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "uid": "mimir", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "unit": "Bps"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(envoy_http_downstream_cx_rx_bytes_total[5m]))",
          "legendFormat": "Bytes In",
          "refId": "A"
        },
        {
          "expr": "sum(rate(envoy_http_downstream_cx_tx_bytes_total[5m]))",
          "legendFormat": "Bytes Out",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Certificate Expiry",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "datasource": { "uid": "mimir", "type": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 604800 },
              { "color": "green", "value": 2592000 }
            ]
          },
          "custom": {
            "displayMode": "color-background"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [{ "id": "custom.hidden", "value": true }]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [{ "id": "unit", "value": "dtdurations" }]
          }
        ]
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "reduce",
          "options": { "reducers": ["lastNotNull"] }
        }
      ],
      "targets": [
        {
          "expr": "(certmanager_certificate_expiration_timestamp_seconds - time())",
          "legendFormat": "{{name}} ({{namespace}})",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["nebari", "envoy", "gateway"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Nebari Gateway Traffic",
  "uid": "nebari-gateway-traffic",
  "version": 1
}
